<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="0.0dip" />
            <gradient android:startColor="#4cffffff" android:endColor="#4cffffff" android:angle="270.0" android:centerY="0.45" android:centerColor="#4cffffff" />
        </shape>
    </item>
     <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="0.0dip" />
                <gradient android:startColor="#FFDAB9" android:endColor="#FFDAB9" android:angle="270.0" android:centerY="0.45" android:centerColor="#FFDAB9" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="0.0dip" />
                <gradient android:startColor="#ff167efc" android:endColor="#ff167efc" android:angle="270.0" android:centerY="0.45" android:centerColor="#ff167efc" />
            </shape>
        </clip>
    </item>
</layer-list>
